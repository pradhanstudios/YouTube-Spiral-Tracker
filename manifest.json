{
    "name": "YouTube Spiral Tracker",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://youtube.com/*"
    ]
}
